regist_master_model:
  recipe_id:			# レシピ名
  cropper:
    zcrop:			# Z方向クロップパラメータ（cropperから取得する）
      near:			# Z方向クロップ位置（カメラ側）
      far:			# Z方向クロップ位置（床側）
    sphare:			# 円クロップのパラメータ（cropperから取得する）
      x:			# 円の中心X座標
      y:			# 円の中心Y座標
      r:			# 半径
    voxcel:			# voxelサイズ(0の場合はvoxel化なし)
    frame_id:			# 出力座標系
  ransac:			# ransacパラメータ
    fitness_threshold:		# ransacを終了するfitness閾値
    max_iteration:		# 反復の最大回数
    max_validation:		# 検証ステップの最大数
  icp:				# ICPパラメータ
    fitness_threshold:		# ICP一致率閾値（この値未満の場合にはICPの結果NGを返す）
    translation_threshold:	# ICP算出位置をロボットに通知する際の閾値(組となっているupper,lowerが両方0の場合には閾値判定しない)
      upper_x:			# X座標上限
      lower_x:			# X座標下限
      upper_y:			# Y座標上限
      lower_y:			# Y座標下限
      upper_z:			# Z座標上限
      lower_z:			# Z座標下限
    rotation_euler_threshold:	# ICP算出位置をロボットに通知する際の閾値(組となっているupper,lowerが両方0の場合には閾値判定しない)
      upper_a:			# X回転上限
      lower_a:			# X回転下限
      upper_b:			# Y回転上限
      lower_b:			# Y回転下限
      upper_c:			# Z回転上限
      lower_c:			# Z回転下限
  camera:			# カメラパラメータ
    exposure_time:		# 露光時間
    gain:			# ゲイン
  projector:			# プロジェクターパラメータ
    exposure_time:		# 露光時間
    intencity:			# 発光強度
    interval:			# 発光間隔
  algor:			# マッチングアルゴリズム(1:ICP その他は未定)
